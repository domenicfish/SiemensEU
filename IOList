                                <span data-show-if="material5.Is_Hide_Unit_Sales != 1 AND Custom1__c_Generic_DocGen__r_Price_levels__c LIKE 'Item'">
                                  <span data-show-if="material5.Is_Option = 1">(</span>
                                  <span data-show-if="material5.type = 'SP' OR material5.Is_Service = 1">
                                    <span data-show-if="Custom1__c_Generic_DocGen__r_With_services__c LIKE 'false' AND material5.Mat_Serv_Sales = 0"></span>
                                    <span class="number" data-show-if="Custom1__c_Generic_DocGen__r_With_services__c LIKE 'true' AND material5.Mat_Serv_Sales != 0">[material5.Mat_Serv_Sales / material5.Serv_Calc_Qty]</span>
                                  </span>
                                  <span data-show-if="material5.type != 'SP' AND material5.Is_Service != 1">
                                    <span class="number" data-show-if="material5.Mat_Calc_Unit_Sales != 0">[material5.Mat_Calc_Unit_Sales]</span>
                                    <span data-show-if="material5.Mat_Calc_Unit_Sales = 0"></span>
                                  </span>
                                  <span data-show-if="material5.Is_Option = 1">)</span>
                                </span>
                              </td>
                              <!--total net price column material5-->
                              <td data-show-if="Custom1__c_Generic_DocGen__r_Price_Varient__c LIKE 'Net' OR Custom1__c_Generic_DocGen__r_Price_Varient__c = 'Gross Price'" style="width: 9%; background-color: #ffffff; vertical-align: top; text-align: right;" class="number">
                                <span data-show-if="material5.Is_Hide_Tot_Sales != 1 AND Custom1__c_Generic_DocGen__r_Price_levels__c LIKE 'Item'">
                                  <span data-show-if="material5.Is_Option = 1">(</span>
                                  <span data-show-if="material5.type = 'SP' OR material5.Is_Service = 1">
                                    <span data-show-if="Custom1__c_Generic_DocGen__r_With_services__c LIKE 'false'"></span>
                                    <span class="number" data-show-if="Custom1__c_Generic_DocGen__r_With_services__c LIKE 'true' AND material5.Mat_Serv_Sales != 0">[material5.Mat_Serv_Sales]</span>
                                  </span>
                                  <span data-show-if="material5.type != 'SP' AND material5.Is_Service != 1">
                                    <span data-show-if="material5.Mat_Calc_Unit_Sales_hidden = 0"></span>
                                    <span class="number" data-show-if="material5.Mat_Calc_Unit_Sales_hidden != 0">[material5.Mat_Calc_Unit_Sales_hidden]</span>
                                  </span>
                                  <span data-show-if="material5.Is_Option = 1">)</span>
                                </span>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
